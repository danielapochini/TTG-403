	.links{
		margin:15px;
	}
	
	.links a{
		border-radius:10px;
		position:relative;
		top:0;
		transition: all .2s ease-in-out;
		color:#008;
	}
	.links a:hover{
		text-decoration:none;
		top:-4px;
		color:#000080;
		font-weight:normal;
		background-color:transparent;
		box-shadow:0 4px 3px #999
	}
	iframe{
		border-radius:20px;
	}
	
	footer{	
		font-size:14px;		
		border-radius:10px;
		background-color:rgba(68, 68, 119,0.85);/*#444477; */
		color:#ccc;
		font-weight:bold;
		padding:10px;
	}
	
	.lado1{
		float:left;
	}
	
	.lado2{
		float:right;
	}
	
	.sv1{
		margin-bottom: 30px;
	}